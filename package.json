{
  "name": "@jsoncut/mcp-server",
  "version": "1.3.0",
  "description": "Model Context Protocol server for jsoncut API - Generate images and videos using AI",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "jsoncut-mcp": "./dist/index.js",
    "jsoncut-mcp-http": "./dist/http-server.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js",
    "start:http": "node dist/http-server.js",
    "dev:http": "tsc && node dist/http-server.js",
    "docker:build": "docker build -t centerbit/jsoncut-mcp-server:local .",
    "docker:run": "docker-compose up",
    "docker:stop": "docker-compose down",
    "docker:publish": "./publish-docker.sh"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "jsoncut",
    "video-generation",
    "image-generation",
    "media-processing",
    "ai",
    "llm",
    "claude",
    "cursor"
  ],
  "author": "Jsoncut Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsoncut/jsoncut-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/jsoncut/jsoncut-mcp-server/issues"
  },
  "homepage": "https://github.com/jsoncut/jsoncut-mcp-server#readme",
  "files": [
    "dist",
    "schemas",
    "examples",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "SETUP.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
